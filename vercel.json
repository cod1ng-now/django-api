{
    "builds": [
      {
        "src": "app/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "api/wsgi.py"
      }
    ]
  }